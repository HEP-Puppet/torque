# server commands
<% @sconfig.each do |value| -%>
<%= value %>
<% end -%>
# queue commands
<% @qconfig.keys.sort.each do |queue| -%>
<%	@qconfig[queue].each do |value|
		if value =~ /^create queue / -%>
<%= value %>
<%		else -%>
set queue <%= queue %> <%= value %>
<%		end
	end
end -%>
